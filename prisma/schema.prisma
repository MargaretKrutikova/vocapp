datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

type VocItem {
    value    String
    language String
}

model VocValue {
    id           String    @id @default(auto()) @map("_id") @db.ObjectId
    tenant       String?
    value        String
    language     String
    translations VocItem[]
    explanations VocItem[]
    usages       VocItem[]
}
